{
  "name": "firestore-fp",
  "version": "0.1.0",
  "author": {
    "name": "Denis Goncharenko",
    "email": "dddenjer@gmail.com"
  },
  "license": "MIT",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "engines": {
    "node": "14"
  },
  "sideEffects": false,
  "packageManager": "yarn@3.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/dddenis/firestore-fp.git"
  },
  "bugs": {
    "url": "https://github.com/dddenis/firestore-fp/issues"
  },
  "homepage": "https://github.com/dddenis/firestore-fp",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf ./build",
    "dev": "docker-compose --file ./dev/docker-compose.yml --project-name @dddenis/firestore-fp",
    "format": "yarn format:base .",
    "format:base": "prettier --config ./.prettierrc.ci.js --write",
    "lint": "yarn lint:base .",
    "lint:base": "eslint --config .eslintrc.ci.js --max-warnings=0",
    "postinstall": "husky install"
  },
  "dependencies": {
    "dataloader": "^2.0.0",
    "ts-toolbelt": "^9.0.0"
  },
  "devDependencies": {
    "@google-cloud/firestore": "4.15.1",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "fp-ts": "2.11.2",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "prettier": "2.4.0",
    "prettier-plugin-organize-imports": "2.3.3",
    "typescript": "4.4.3"
  },
  "peerDependencies": {
    "@google-cloud/firestore": "^4.0.0",
    "fp-ts": "^2.11.0"
  }
}
